#f) Implement Python Script to generate prime numbers series up to n

def is_prime(num):
    if num <= 1:
        return False
    for i in range(2, int(num ** 0.5) + 1):  # Check up to the square root of num
        if num % i == 0:
            return False
    return True

n = int(input("Enter the value of n: "))

print(f"Prime numbers up to {n} are:")
for i in range(2, n + 1):
    if is_prime(i):
        print(i, end=' ')




